<svg width="<%= width %>" viewBox="0 0 1020 140" fill="none" xmlns="http://www.w3.org/2000/svg">
    <% contributions.slice(-357).forEach((contribution, i) => { %>
        <rect width='20' height='20' fill='<%= getColor(contribution) %>' x='<%= Math.floor(i / 7) * 20 %>' y='<%= (i % 7) * 20 %>'>
            <text><%= contribution.date %></text>
        </rect>
    <% }) %>

    <style type="text/css">
      @font-face {
        font-family: <%= font %>;
        font-style: normal;
        font-weight: 400;
        src: url('../fonts/<%= font %>.eot'); /* IE9 Compat Modes */
        src: local(''),
        url('/fonts/<%= font %>/<%= font %>.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
        url('/fonts/<%= font %>/<%= font %>.woff2') format('woff2'), /* Super Modern Browsers */
        url('/fonts/<%= font %>/<%= font %>.woff') format('woff'), /* Modern Browsers */
        url('/fonts/<%= font %>/<%= font %>.ttf') format('truetype'), /* Safari, Android, iOS */
        url('/fonts/<%= font %>/<%= font %>.svg#RobotoCondensed') format('svg'); /* Legacy iOS */
      }
    </style>

    <text
        fill="<%= fontColor %>"
        font-size="48"
        letter-spacing="0em"
        text-anchor="middle"
        x="50%"
        y="60%"
        font-family="<%= font %>, sans-serif"
    >
        <%= name %>
    </text>

</svg>
